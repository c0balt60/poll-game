		"fix": "eslint --fix places/* ",
		"prepare": "husky",
		"watch": "rbxtsc -w",
		"clean": "rm -rf out && find places -type d -name 'out' -exec rm -rf {} +",
		"compile:base": "cd places/base && rbxtsc --verbose && darklua process out dist --verbose",
		"watch:base": "cd places/base && rbxtsc -w --verbose",
		"build:base": "rojo build -o places/base/build.rbxl places/base && lune run sync-models places/base/build.rbxl base",
		"prod:base": "rojo build -o places/base/build.rbxl places/base/production.project.json && lune run sync-models places/base/build.rbxl base",
		"open:base": "xdg-open places/base/build.rbxl",
		"extract:base": "lune run extract-models places/base/build.rbxl base",
		"test:base": "bun clean && bun extract:base && bun compile:base && bun build:base && lune run client-tests places/base/build.rbxl && lune run server-tests places/base/build.rbxl && lune run client-tests places/base/build.rbxl"
  },
